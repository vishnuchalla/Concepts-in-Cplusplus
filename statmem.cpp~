#include<iostream>
using std::cout;
using std::cin;
class Sum
{
 static int a;
 int b;
 public:
 void add()
 {
  b=0;
  a=a++;
  b=b++;
  cout<<"a="<<a<<"\tb="<<b;
 }
};
int Sum::a=9;
main()
{
 Sum s1,s2;
 cout<<"\nvalues of a&b in object s1";
 s1.add();
 cout<<"\nvalues of a&b in object s2";
 s2.add();
}
 
